{
    "name": "jubstuff/try-codeception",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Giustino Borzacchiello",
            "email": "giustinob@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.11",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan-shim": "^0.9",
        "codeception/codeception": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Jubstuff\\TryCodeception\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jubstuff\\TryCodeception\\": [
                "tests/",
                "tests/Fake"
            ]
        }
    },
    "scripts": {
        "acceptance-php": [
            "vendor/bin/codeception run acceptance-php --steps"
        ],
        "acceptance-web": [
            "vendor/bin/codeception run acceptance-web --steps"
        ],
        "server": [
            "php -S localhost:8000 -t public"
        ]
    }
}
